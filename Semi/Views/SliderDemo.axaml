<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800"
             d:DesignHeight="450"
             x:Class="Semi.Views.SliderDemo">
    <StackPanel HorizontalAlignment="Left" Spacing="30">
        <Slider IsEnabled="False" Width="300" Minimum="0" Maximum="100" Value="30"></Slider>
        <Slider IsEnabled="True" Width="300" Minimum="0" Maximum="100" Value="30"></Slider>
    </StackPanel>
</UserControl>
